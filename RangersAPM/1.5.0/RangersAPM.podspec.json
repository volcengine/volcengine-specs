{
    "name": "RangersAPM",
    "version": "1.5.0",
    "summary": "RangersAPM by ByteDance",
    "homepage": "https://apm.bytedance.com",
    "license": {
        "type": "MIT",
        "file": "RangersAPM/LICENSE"
    },
    "authors": "ByteDance",
    "platforms": {
        "ios": "8.0"
    },
    "frameworks": [
        "UIKit",
        "AdSupport"
    ],
    "dependencies": {
        "RangersAppLog/Core": [
            ">=5.0.0"
        ]
    },
    "pod_target_xcconfig": {
        "DEFINES_MODULE": "YES"
    },
    "user_target_xcconfig": {
        "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
    },
    "static_framework": true,
    "preserve_paths": "**/*",
    "subspecs": [
        {
            "name": "Public",
            "source_files": "RangersAPM/Public/**/*.{h,m}",
            "public_header_files": "RangersAPM/Public/**/*.h",
            "vendored_libraries": "RangersAPM/Public/**/*.a",
            "dependencies": {
                "RangersAPM/Core": []
            }
        },
        {
            "name": "Core",
            "vendored_libraries": "RangersAPM/Core/**/*.a",
            "libraries": [
                "c++",
                "z",
                "sqlite3"
            ]
        },
        {
            "name": "Crash",
            "source_files": "RangersAPM/Crash/**/*.{h,m}",
            "public_header_files": "RangersAPM/Crash/**/*.h",
            "vendored_libraries": "RangersAPM/Crash/**/*.a",
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/Public": [],
                "RangersAPM/HMD": []
            },
            "resources": [
                "RangersAPM/Assets/Crash/**/APMInsightCrash.bundle"
            ],
            "libraries": "c++abi"
        },
        {
            "name": "WatchDog",
            "vendored_libraries": "RangersAPM/WatchDog/**/*.a",
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/HMD": [],
                "RangersAPM/Public": []
            }
        },
        {
            "name": "OOM",
            "vendored_libraries": "RangersAPM/OOM/**/*.a",
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/Crash": [],
                "RangersAPM/WatchDog": [],
                "RangersAPM/HMD": [],
                "RangersAPM/Public": []
            }
        },
        {
            "name": "HMD",
            "vendored_libraries": "RangersAPM/HMD/**/*.a",
            "dependencies": {
                "RangersAPM/Core": []
            }
        },
        {
            "name": "LAG",
            "vendored_libraries": "RangersAPM/LAG/**/*.a",
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/HMD": [],
                "RangersAPM/Public": []
            }
        },
        {
            "name": "UserException",
            "source_files": "RangersAPM/UserException/**/*.{h,m}",
            "public_header_files": "RangersAPM/UserException/**/*.h",
            "vendored_libraries": "RangersAPM/UserException/**/*.a",
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/HMD": [],
                "RangersAPM/Public": []
            }
        },
        {
            "name": "UITrackers",
            "vendored_libraries": "RangersAPM/UITrackers/**/*.a",
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/HMD": [],
                "RangersAPM/Public": []
            }
        },
        {
            "name": "Monitors",
            "source_files": "RangersAPM/Monitors/**/*.{h,m}",
            "vendored_libraries": "RangersAPM/Monitors/**/*.a",
            "public_header_files": "RangersAPM/Monitors/**/*.h",
            "dependencies": {
                "RangersAPM/UITrackers": []
            }
        },
        {
            "name": "Hybrid",
            "vendored_libraries": "RangersAPM/Hybrid/**/*.a",
            "resource_bundles": {
                "APMInsightHybrid": [
                    "RangersAPM/Assets/Hybrid/**/*"
                ]
            },
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/HMD": [],
                "RangersAPM/Public": []
            },
            "frameworks": "WebKit"
        },
        {
            "name": "MemoryGraph",
            "vendored_libraries": "RangersAPM/MemoryGraph/**/*.a",
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/HMD": [],
                "RangersAPM/Public": [],
                "SSZipArchive": []
            }
        },
        {
            "name": "Network",
            "vendored_libraries": "RangersAPM/Network/**/*.a",
            "dependencies": {
                "RangersAPM/Core": [],
                "RangersAPM/HMD": [],
                "RangersAPM/Public": []
            },
            "libraries": "c++"
        }
    ],
    "source": {
        "http": "https://lf1-ttcdn-tos.pstatp.com/obj/heimdallr/RangersAPM/1.5.0/RangersAPM.zip"
    }
}